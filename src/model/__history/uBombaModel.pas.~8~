unit uBombaModel;

interface

type
  TBomba = class
    public
      property  ID          : integer read  FID           write SetID;
      property  DESCRICAO   : string  read  FDESCRICAO    write SetDESCRICAO;
      property  ID_TANQUE   : integer read  FID_TANQUE    write SetID_TANQUE;

      Constructor Create;
  end;

implementation

end.
